{
  "name": "applied-ai-systems-website",
  "version": "1.0.0",
  "private": true,
  "description": "Official website for Applied AI Systems LLC - Government contractor specializing in AI, space physics, and RF systems",
  "homepage": "https://applied-ai-systems.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/applied-ai-systems/aas-website-public"
  },
  "author": "Applied AI Systems LLC",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "clean": "rm -rf .next out coverage .lighthouseci",
    "analyze": "cross-env ANALYZE=true next build",
    "security:scan": "npm audit && snyk test"
  },
  "dependencies": {
    "autoprefixer": "^10.4.21",
    "next": "^15.5.2",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "15.1.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.0",
    "postcss": "^8",
    "prettier": "^3.0.0",
    "snyk": "^1.1200.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "applied-ai-systems",
    "government-contractor",
    "ai",
    "space-physics",
    "rf-systems",
    "data-engineering",
    "nextjs",
    "typescript",
    "aerospace",
    "defense"
  ]
}
